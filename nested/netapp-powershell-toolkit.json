{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "type": "string"
        },
        "_artifactsLocationSasToken": {
            "type": "securestring"
        },
        "sqlVmName": {
            "type": "string"
        },
        "NetAppTags": {
            "type": "object"
        },
        "quickstartTags": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [{
        "type": "Microsoft.Compute/virtualMachines/extensions",
        "name": "[concat(parameters('sqlVmName'),'/')]",
        "apiVersion": "2016-03-30",
        "location": "[resourceGroup().location]",
        "tags": {
            "quickstartName": "[parameters('quickstartTags').name]",
            "provider": "[parameters('NetAppTags').provider]"
        },

        "properties": {
            "publisher": "Microsoft.Compute",
            "type": "CustomScriptExtension",
            "typeHandlerVersion": "1.8",
            "autoUpgradeMinorVersion": true,
            "settings": {
                "fileUris": [
                    "[concat(parameters('_artifactsLocation'),'/scripts/NetApp_PowerShell_Toolkit_4.3.0.msi')]"
                ],
                "commandToExecute": "msiexec.exe /a NetApp_PowerShell_Toolkit_4.3.0.msi"
            }
        }

    }]
}